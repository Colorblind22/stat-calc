<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stat.NormCdf">
    <children>
        <Label text="Normal Cdf" />
        <TextField fx:id="input" promptText="Data separated by spaces (i.e.: 4 5 6...)"/>
        <TextField fx:id="lb_input" promptText="Lower Bound"/>
        <TextField fx:id="ub_input" promptText="Upper Bound"/>

        <Button fx:id="calculateNormCdf" text="Calculate normCdf" onAction="#normCdf_data"/>
        <Label fx:id="label" text=" "/>
        <Button text="Back" onAction="#backToNormCdfSelect"/>
    </children>
    
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

</VBox>